doctype 5
html.wf-ffmetaserifwebpro-n5-active.wf-ffmetaserifwebpro-n7-active.wf-ffmetawebpro-n4-active.wf-active(lang='en')
    head
        title Jarvis Analytics
        link(rel='stylesheet', href='/assets/css/bootstrap.css')
        link(rel='stylesheet', href='/assets/css/bootstrap-responsive.css')
        link(rel='stylesheet', href='/assets/css/analytics-docs.css')
        link(rel='stylesheet', href='/assets/css/jarvis.css')
        link(rel='stylesheet', href='/assets/css/analytics-site.css')
        link(rel='stylesheet', href='http://use.typekit.net/c/bc8202/ff-meta-serif-web-pro:n5:n7,ff-meta-web-pro:n4.VbN:M:2,VbQ:M:2,ZG3:M:2/d?')
        script(src='http://use.typekit.net/rvl8eqz.js')
        script(src='/assets/js/jquery-1.7.2.min.js')
        script(src='/assets/js/jquery-ui.min.js')
        script(src='/assets/js/html5.js')
        script(src='/assets/js/bootstrap.min.js')
        script(src='/assets/js/prettify.js')
        script(src='/assets/js/analytics-site.js')
    body
        div.navbar.navbar-inverse.navbar-static-top
            div.navbar-inner
                div._container
                    a.brand(href='/', style='line-height: 26px; margin-top: 10px !important;margin-left: 0;') Jarvis Analytics
                        br
                        span(style='float:right;font-size:82%;font-weight:normal;color:#5d95fa') beta
                    div.topnav
                        ul.nav.pull-right.loginbox(style='margin-right:0;')
                            li#Li2.dropdown
                                a.dropdown-toggle.loginname(href='#', data-toggle='dropdown')
                                //    strong.caret
                                //ul.dropdown-menu
                                    li.defaultnav
                                        a(href='/manage', style='text-transform:none;') Manage
                                    li.defaultnav
                                        a(href='/docs') Documentation
                                    li.defaultnav
                                        a(href='/support') Support
                                    li.divider
                                    li.defaultnav
                                        a(href='/logout') Log out
        div.sep
        div.bodywrapper
            table.table_wrapper
                tr
                    td.leftnav(style='padding:0;')
                        div
                            div.searchmore_wrapper
                                input.searchmore_text(type='text', size='24', label='Find reports & more', placeholder='Find reports & more')
                            div.popup_wrapper(style='display:none')
                                div.recent_wrapper
                                    div.recentlyviewedcaption Recently viewed reports
                        div
                            div.menu_wrapper.listcontainer
                                div.maintopic
                                div.topic.topic-dashboards
                                    div.topicheader
                                        div.colorspacer
                                        div.topicicon
                                        div.topiccaption Dashboards
                                    ul.nav-dashboards.subtopic
                                div.topic.topic-reports
                                    div.topicheader
                                        div.colorspacer
                                        div.topicicon
                                        div.topiccaption Reports
                                    ul.nav-reports.subtopic
                            //div.helpblock
                                div.reporthelp
                                    div.jarvis.caption
                                    div.jarvis.description
                                div.resources
                                div.resources
                                    div.resourcewrapper
                                        a.resource Help center

                    td.navsep
                        div(style='min-width:5px;width:5px')
                    td.content_wrapper
                        div.overlay(style='height:1500px;width: 100%;opacity:0.5;display:block;overflow:hidden;')
                            img(src='/assets/img/spinner-loading.gif')
                        div.container.jarvis._container(style='display:block')
                            div.filtersWrapper.row.fluid-row
                                div.filterWrapper
                                    div.filterContainer
                            header#overview.jumbotron.subhead
                                div.jarvis.picker.datebox(data-format='mmm dd, yyyy hh:nn:ss', data-offsetx='-55', data-offsety='59')
                                h1.jarvis.caption
                                p.jarvis.description.lead
                                div.subnav(style='display:block')
                                    ul.nav.nav-pills
                                        li.widget-add.pull-right(style='display:none')
                                            a +Add Widget
                                        li.divider.pull-right
                                        li.panel-edit.pull-right(style='display:none')
                            div#dashboardpanel.jarvis.dashboard.panel(style='display:none;')
                                div.filtersWrapper.row.fluid-row
                                div.jarvis.dashboard.widgets
                            div#reportpanel.jarvis.report.panel(style='display:none')
                                div.jarvis.report.tabs
                                div.report_wrapper
                                    div.jarvis.report.metricgroups
                                    div.jarvis.report.row-top.row.fluid-row
                                        div
                                            div.jarvis.report.timeline
                                    div.jarvis.report.row-middle.row.fluid-row(style='padding-top:20px;')
                                        div.metricswrapper
                                            div.jarvis.report.metricbox
                                    div.jarvis.report.row-bottom.row.fluid-row(style='margin-top:20px;padding-top:0px;border-top:0px solid #ccc;')
                                        div
                                            div.jarvis.report.jtable
        div.sep
        footer.footer
            div.container
               // div.row
                    div.span8
                        h3.bottomcaption Joola Analytics
                        div.row
                            div.span2
                                p Product
                                ul
                                    li
                                        a(href='') What is Joola Analytics?
                                    li
                                        a(href='') How does it work?
                                    li
                                        a(href='') Get started
                                    li
                                        a(href='') Blog
                            div.span2
                                p Company
                                ul
                                    li
                                        a(href='') About
                                    li
                                        a(href='') Contact Us
                                    li
                                        a(href='') Terms of Service
                            div.span2
                                p Developers
                                ul
                                    li
                                        a(href='') Overview
                                    li
                                        a(href='') FAQ
                                    li
                                        a(href='') Documentation
                                    li
                                        a(href='') Playground
                                    li
                                        a(href='') Support
                    div.span3
                        h3.bottomcaption &nbsp;
                        p Follow us
                        ul
                            li
                                a(href='') @jarvisanalytics

                p(style='font-weight:normal;margin-top:50px;') Built using 
                    a(href='') Joola 
                    | Technology Â© 2013 
                    a(href='') Joola Smart Solutions Ltd. 
                    | All rights reserved 
                    a(href='') Terms and Conditions
    script(src='https://www.google.com/jsapi', type='text/javascript')
    script(src='/joola.min.js?token=#{jarvisToken}', type='text/javascript')
    script(type='text/javascript').
        google.load('visualization', '1.0', {packages: ['corechart']});
            google.load("visualization", '1.0', {packages: ['imagechart']});

            $(window).bind('jarvis-loaded', function () {
                setupLeftNavContents();

                jarvis.bootstrap = true;
                jarvis.visualisation.init();
                $('.jarvis.version').text('Version ' + jarvis.VERSION);
                });

